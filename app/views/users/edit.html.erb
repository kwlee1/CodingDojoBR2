<a href="/events">Event Dashboard</a> 
<form class="" action="/sessions" method="post">
    <input type="hidden" name="authenticity_token" value="<% form_authenticity_token %>">
    <input type="hidden" name="_method" value="delete">
    <input type="submit" value="Logout">
</form>
<h1>Edit Your Profile</h1>
<% if flash[:errors] %>
    <% flash[:errors].each do |e| %>
        <p><%= e %></p>
    <% end %>
<% end %>
<form action="/users" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    First Name: <input type="text" name="user[first_name]" value="<%= current_user.first_name %>"><br>
    Last Name: <input type="text" name="user[last_name]" value="<%= current_user.last_name %>"><br>
    Email: <input type="text" name="user[email]" value="<%= current_user.email %>"><br>
    City: <input type="text" name="user[city]" value="<%= current_user.city %>"><br>
    State: <input type="text" name="user[state]" value="<%= current_user.state %>"><br>
    <input type="submit" value="Register">
</form>